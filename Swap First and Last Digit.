#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<stdio.h>
#include<math.h>
int main(){
    int n,first,last,temp,count=0;
    scanf("%d",&n);

    if(n>=0 && n<=9){
        printf("%d",n);
    }else{
        temp=n;
        last=n%10;
        
        while(temp!=0){
            temp=temp/10;
            count++;
        }
        first= n / (pow(10,count-1));

        int middle = (n % (int)pow(10 ,count-1))/10;

        int swapped=last*pow(10,count-1)+middle*10+first;
        printf("%d",swapped);
    }

    return 0;
}
