#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main(){
    int n;
    scanf("%d",&n);
    int list[n];
    for(int i=0;i<n;i++){
        scanf("%d",&list[i]);
    }
    int val,pos;
    scanf("%d",&val);
    scanf("%d",&pos);
    
    for(int i=n-1;i>=pos;i--){
        list[i+1]=list[i];
        }
        list[pos]=val;
        n++;
    
    
    for(int i=0;i<n;i++){
        printf("%d ",list[i]);
    }
    return 0;
}
